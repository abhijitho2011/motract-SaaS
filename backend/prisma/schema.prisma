generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"

}

enum Role {
  SUPER_ADMIN
  WORKSHOP
  CLIENT
  SUPPLIER
  RSA_PROVIDER
  SPECIALIST
}

model User {
  id        String   @id @default(uuid())
  email     String   @unique
  password  String
  role      Role
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
